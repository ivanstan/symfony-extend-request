{
  "name": "ivanstan/symfony-extend-request",
  "description": "Adds ability to extend Symfony request class and use child class as controller method parameter type",
  "type": "symfony-bundle",
  "require": {
    "php": ">=8.0",
    "symfony/framework-bundle": "^4.0|5.0|^6.0",
    "symfony/yaml": "^4.0|5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Ivanstan\\SymfonyExtendRequest\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Stanojevic",
      "email": "ivanstan@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpunit/php-code-coverage": "^9.2",
    "symfony/test-pack": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Ivanstan\\SymfonyExtendRequest\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "php -dxdebug.mode=coverage ./vendor/bin/phpunit --coverage-text --coverage-html tests/coverage"
  }
}
